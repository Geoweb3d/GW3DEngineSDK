<GDAL_WMTS>
	<GetCapabilitiesUrl>https://geoint.nrlssc.navy.mil/dnc/wmts/DNC-WORLD/basemap/DNC-BASEMAP-OVERLAY?REQUEST=GetCapabilities&VERSION=1.0.0&SERVICE=WMTS</GetCapabilitiesUrl>
	<Layer>DNC-BASEMAP-OVERLAY</Layer>
	<Style>default</Style>
	<TileMatrixSet>GlobalCRS84Pixel</TileMatrixSet>
	<Format>image/png</Format>
	<DataWindow>
		<UpperLeftX>-180</UpperLeftX>
		<UpperLeftY>90</UpperLeftY>
		<LowerRightX>179.9999999999998</LowerRightX>
		<LowerRightY>-89.99999999999989</LowerRightY>
	</DataWindow>
	<BandsCount>3</BandsCount>
	<Cache>
		<Path></Path>
		<MaxSize>64</MaxSize>
		<Expires>604800</Expires>
	</Cache>
	<UnsafeSSL>true</UnsafeSSL>
	<ZeroBlockHttpCodes>204,404</ZeroBlockHttpCodes>
	<ZeroBlockOnServerException>true</ZeroBlockOnServerException>
	<MaxConnections>5</MaxConnections>	
</GDAL_WMTS>
