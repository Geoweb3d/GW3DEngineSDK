<GDAL_WMS>
	<Service name="VirtualEarth">
		<ServerUrl>http://a${server_num}.ortho.tiles.virtualearth.net/tiles/a${quadkey}.jpeg?g=90</ServerUrl>
	</Service>	
	<Cache>
		<Path></Path>
		<MaxSize>64</MaxSize>
		<Expires>604800</Expires>
	</Cache>
	<MaxConnections>4</MaxConnections>
	<DataWindow>
	<!--will fail after 20-->
	<TileLevel>20</TileLevel>
	</DataWindow>
</GDAL_WMS>